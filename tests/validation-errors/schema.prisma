// ========================
// Generator Tests
// ========================

generator typescriptInterfaces {
  provider              = "tsx src/generator.ts"
  modelType             = "bad"
  enumType              = incorrect
  typeImportPath        = [["nested"], type]
  intType               = ["wrong"]
  wrongOption           = ["an", option, ""]
  stringType            = []
  booleanType           = ""
  badOption             = "not"
  exportEnums           = "this"
  optionalRelations     = is
  omitRelations         = "not"
  optionalNullables     = "a"
  prettier              = boolean
  resolvePrettierConfig = "value"
}

// ========================
// Prisma Schema
// ========================

datasource db {
  provider = "postgresql"
  url      = ""
}

model User {
  id Int @id
}
